          <div class="sidebar-filter mt-50">
            <div class="top-filter-head">Product Filters</div>
            <div class="common-filter">
              <div class="head">Brands</div>
              <form action="#">
                <ul>
                  <% brandsSet = new Set(); %>
                  <% products.forEach((product, index) => { brandsSet.add(product.brand); }); %>
                  <% const uniqueBrands = Array.from(brandsSet); %>
                  <% uniqueBrands.forEach((brand, index) => { %>
                  <li class="filter-list">
                    <input
                      class="pixel-radio"
                      type="radio"
                      id="brand<%= index %>"
                      name="brand"
                      value="<%= brand %>"
                    />
                    <label for="brand<%= index %>">
                      <%= brand %>
                      <span>(<%= products.filter(product => product.brand === brand).length %>)</span>
                    </label>
                  </li>
                  <% }) %>
                </ul>
              </form>
            </div>
          
            <div class="common-filter">
              <div class="head">Color</div>
              <form action="#">
                <ul>
                  <% const colorsSet = new Set(products.map(product => product.color)); %>
                  <% const uniqueColors = Array.from(colorsSet); %>
                  <% uniqueColors.forEach((color, index) => { %>
                  <li class="filter-list">
                    <input
                      class="pixel-radio"
                      type="radio"
                      id="color<%= index %>"
                      name="color"
                      value="<%= color %>" 
                    />
                    <label for="color<%= index %>">
                      <%= color %>
                      <span>(<%= products.filter(product => product.color === color).length %>)</span>
                    </label>
                  </li>
                  <% }) %>
                </ul>
              </form>
            </div>
          
            <div class="mt-5">
              <center>
                <button class="btn btn-primary bg-dark" onclick="filterHandler()">Start to filter</button>
              </center>
            </div>
          </div>
          
