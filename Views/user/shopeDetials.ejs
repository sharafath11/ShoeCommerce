<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>



<script>
  let datas = JSON.parse(document.getElementById("products").value);
  const productsPerPage = 9;

  function addToWishList(productId) {
      axios.post(`/whislistAdd/${productId}`)
          .then(function(response) {  
              if (response.data.success) {
                  showToast(response.data.message);
                  return addWishlistQty();
              } else {
                  if (response.data.message) {
                      return showToast(response.data.message);
                  } else {
                      window.location.href = '/ShowLoginMsg';
                  }
              }
          })
          .catch(function(error) {
              showToast('Please login :)');
              console.log(error);
          });
  }

  function addWishlistQty() {
      let currentWtQty = parseInt(document.getElementById('w-qty').innerText);
      document.getElementById('w-qty').innerText = currentWtQty + 1;
  }

  function showToast(message) {
      Toastify({
          text: message,
          duration: 3000,
          gravity: "top",
          position: "right",
          backgroundColor: "#333",
          stopOnFocus: true
      }).showToast();
  }

  function filterHandler() {
      const selectedBrand = document.querySelector('input[name="brand"]:checked')?.value;
      const selectedColor = document.querySelector('input[name="color"]:checked')?.value;

      if (!selectedBrand && !selectedColor) {
          alert('Please select at least one filter.');
          return;
      }

      const filterData = {
          brand: selectedBrand || null,
          color: selectedColor || null
      };

      axios.post('/filter-products', filterData)
          .then(response => {
              const products = response.data;
              if (products.length === 0) {
                  return showToast("This filter product are not our products please filter another.");
              }

              datas = products;
              const totalPages = Math.ceil(datas.length / productsPerPage);
              renderProducts(datas.slice(0, productsPerPage), 1, totalPages); // Render first page
          })
          .catch(error => {
              console.error('Error:', error);
          });
  }

  function handlePriceFilter() {
      const selectElement = document.getElementById("mySelect");
      const selectedValue = selectElement.value;

      if (selectedValue) {
          let sortedData;

          switch (selectedValue) {
              case "2": 
                  sortedData = datas.sort((a, b) => a.price - b.price);
                  break;

              case "3": 
                  sortedData = datas.sort((a, b) => b.price - a.price);
                  break;

              default: 
                  sortedData = datas;
                  break;
          }

          const totalPages = Math.ceil(sortedData.length / productsPerPage);
          renderProducts(sortedData.slice(0, productsPerPage), 1, totalPages); 
      }
  }

  function renderProducts(products, currentPage = 1, totalPages = 1) {
      let productHTML = '';    
      document.getElementById("hidden").style.display = "none";

      products.forEach(product => {
    let sizesHTML = '';

    if (product.availableSize && product.availableSize.length > 0) {
        sizesHTML = '<div class="available-sizes d-flex justify-content-around" ><p>Available Sizes:</p>';
        product.availableSize.forEach(sizeObj => {
            if (sizeObj.stock > 0) { 
                sizesHTML += `[<span class="size">${sizeObj.size}</span>] `;
            }
        });
        sizesHTML += '</div>';
    }

    productHTML += `
        <div class="col-lg-4 col-md-6 mt-5">
            <div class="single-product">
                <a href="/singleprodect/${product._id}">
                    <img class="img-fluid" src="${product.images[0].replace('public', '')}" alt=""/>
                </a>
                <div class="product-details">
                    <h6>${product.name}</h6>
                    <div class="price">
                        <h6>₹${product.price}</h6>
                    </div>
                    
                    <!-- Display available sizes -->
                    ${sizesHTML}

                    <div class="prd-bottom">
                        <div class="social-info" onclick="addToWishList('${product._id}')">
                            <span class="lnr lnr-heart"></span>
                            <p class="hover-text">Wishlist</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    `;
});


      document.getElementById('productList').innerHTML = productHTML;
      renderPagination(currentPage, totalPages);
  }

  function renderPagination(currentPage, totalPages) {
      const paginationContainer = document.querySelector('.pagination');
      paginationContainer.innerHTML = ''; 
      const prevButton = document.createElement('a');
      prevButton.href = '#';
      prevButton.className = 'prev-arrow';
      prevButton.innerHTML = '<i class="fa fa-long-arrow-left" aria-hidden="true"></i>';
      prevButton.onclick = () => changePage(currentPage - 1);
      if (currentPage === 1) prevButton.style.display = 'none'; 
      paginationContainer.appendChild(prevButton);

      for (let i = 1; i <= totalPages; i++) {
          const pageLink = document.createElement('a');
          pageLink.href = '#';
          pageLink.textContent = i;
          pageLink.className = i === currentPage ? 'active' : '';
          pageLink.onclick = () => changePage(i);
          paginationContainer.appendChild(pageLink);
      }

      const nextButton = document.createElement('a');
      nextButton.href = '#';
      nextButton.className = 'next-arrow';
      nextButton.innerHTML = '<i class="fa fa-long-arrow-right" aria-hidden="true"></i>';
      nextButton.onclick = () => changePage(currentPage + 1);
      if (currentPage === totalPages) nextButton.style.display = 'none'; 
  }

  function changePage(page) {
      if (page < 1 || page > Math.ceil(datas.length / productsPerPage)) return;

      const productsToRender = datas.slice((page - 1) * productsPerPage, page * productsPerPage);
      renderProducts(productsToRender, page, Math.ceil(datas.length / productsPerPage));
  }

  document.addEventListener('DOMContentLoaded', () => {
      renderProducts(datas.slice(0, productsPerPage), 1, Math.ceil(datas.length / productsPerPage)); 
  });
  async function searchProducts(event) {
    event.preventDefault(); 
    const query = document.getElementById('search_input').value;
    console.log('====================================');
    console.log(query);
    console.log('====================================');
    if (query.length > 2) { 
      try {
        const response = await axios.get(`/search?query=${query}`);
        const products = response.data.products;

        renderProducts(products)
      } catch (error) {
        console.error('Error fetching search results:', error);
      }
    } else {
      document.getElementById('searchResults').innerHTML = ''; 
    }
  }
</script>

